{
  "name": "order_fetcher",
  "productName": "order_fetcher",
  "version": "1.2.0",
  "appVersion": "20190701",
  "description": "My Electron application description",
  "main": "dist/background.js",
  "scripts": {
    "start": "electron-forge start",
    "dev": "webpack --config webpack.config.js",
    "package": "electron-forge package dist",
    "packager": "electron-packager dist",
    "build": "electron-builder",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": "mac",
  "license": "MIT",
  "build": {
    "appId": "order_fetcher",
    "directories": {
      "app": "./dist",
      "output": "out"
    }
  },
  "config": {
    "forge": {
      "electronPackagerConfig": {
        "dir": "./dist"
      },
      "makers": [{
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "order_fetcher"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "blueimp-md5": "^2.10.0",
    "crypto-js": "^3.1.9-1",
    "css-loader": "^3.1.0",
    "element-ui": "^2.10.1",
    "fs-extra": "^8.1.0",
    "nats": "^1.3.0",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@electron-forge/cli": "^6.0.0-beta.39",
    "@electron-forge/maker-deb": "^6.0.0-beta.39",
    "@electron-forge/maker-rpm": "^6.0.0-beta.39",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.39",
    "@electron-forge/maker-zip": "^6.0.0-beta.39",
    "babel-plugin-component": "^1.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "electron-builder": "^21.1.5",
    "cache-loader": "^4.1.0",
    "copy-webpack-plugin": "^5.0.3",
    "electron": "^5.0.6",
    "electron-squirrel-startup": "^1.0.0",
    "file-loader": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "reactify": "^1.1.1",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "through2": "^3.0.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^2.1.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "vue-auto-routes-webpack-plugin": "^1.0.9",
    "vue-cli-plugin-electron-builder": "^1.3.6",
    "vue-loader": "^15.7.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6"
  }
}